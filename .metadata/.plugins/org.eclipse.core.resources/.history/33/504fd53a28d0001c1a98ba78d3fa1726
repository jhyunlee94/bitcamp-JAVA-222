package Day01;

public class _07_operator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//1. 산술연산자
		int num1 = 10;
		int num2 = 3;
		
		int result = num1 + num2; 
		System.out.printf("%d\n", num1);
		System.out.println("num1 + num2 = " + result);
		
		
		result = num1 + num2; 
		System.out.println("num1 + num2 = " + result);
		
		result = num1 - num2; 
		System.out.println("num1 + num2 = " + result);
		
		result = num1 * num2; 
		System.out.println("num1 + num2 = " + result);
		
		result = num1 / num2; 
		System.out.println("num1 + num2 = " + result);
		
		result = num1 % num2; 
		System.out.println("num1 + num2 = " + result);
		
	}

}
