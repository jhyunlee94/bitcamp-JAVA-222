package Day01;

public class _07_operator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//1. 鈒찗像
		int num1 = 10;
		int num2 = 3;
		
		//num1쎀 num2 오 찗 써쎀
		int result = num1 + num2; 
		System.out.printf("%d\n", num1);
		System.out.println("num1 + num2 = " + result);
		System.out.println("----------------------------");
		
		//num1쎀 num2 짋 찗 써쎀
		result = num1 - num2; 
		System.out.println("num1 - num2 = " + result);
		System.out.println("----------------------------");
		
		//num1쎀 num2 썼 찗 써쎀
		result = num1 * num2; 
		System.out.println("num1 * num2 = " + result);
		System.out.println("----------------------------");
		
		//num1쎀 num2 씱얋 찗 써쎀
		result = num1 / num2; 
		System.out.println("num1 / num2 = " + result);
		System.out.println("----------------------------");
		
		//num1쎀 num2 씱쟵쵔 찗 써쎀
		//num1챹 num2 씱얆 씱쟵쵔 킨
		result = num1 % num2; 
		System.out.println("num1 % num2 = " + result);
		System.out.println("----------------------------");
		
		
		//2. 쥁탺찗像
		int num3 = 10;
		int num4 = -num3;
		System.out.println(num4);
		System.out.println("----------------------------");
		
		
	}

}
