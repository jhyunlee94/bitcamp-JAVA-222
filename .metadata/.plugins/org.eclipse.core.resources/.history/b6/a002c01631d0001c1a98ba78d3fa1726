package Day01;

public class _08_ifElse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//1. if else if else ±¸¹®
		int score = 75;
		
		if(score >=90) {
			System.out.println("A");
		} else if(score >= 80) {
			System.out.println("B");
		} else if(score >= 70) {
			System.out.println("C");
		}else {
			System.out.println("D");
		}
	}

}
